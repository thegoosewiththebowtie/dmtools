<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:controls="using:dmtools.Templates"
             mc:Ignorable="d" d:DesignWidth="1000" d:DesignHeight="700"
             x:Class="dmtools.Views.GensView">
    <TabControl>
        <TabItem>
            <TabItem.Header>
                <TextBlock
                    Text="{DynamicResource npcss}">
                </TextBlock>
            </TabItem.Header>
            <Grid
                ColumnDefinitions="*, 2*, *">
                <Border
                    Grid.Column="0"
                    Grid.ColumnSpan="2"
                    Classes="DefBorder">
                    <Grid
                        ColumnDefinitions="*, 2*">
                    <DataGrid
                        IsReadOnly="True"
                        Name="History"
                        SelectionChanged="History_OnSelectionChanged"
                        Margin="5">
                        <DataGrid.Columns>
                        <DataGridTextColumn
                            Header="{DynamicResource FirstNameG}"
                            Binding="{Binding Path=Name}">
                            
                        </DataGridTextColumn>
                        <DataGridTextColumn
                            Header="{DynamicResource SecondNameG}"
                            Binding="{Binding Path=Name2}">
                            
                        </DataGridTextColumn>
                        <DataGridTextColumn
                            Header="{DynamicResource RaceG}"
                            Binding="{Binding Path=Race}">
                            
                        </DataGridTextColumn>
                    </DataGrid.Columns>
                </DataGrid>
                    </Grid>
                    </Border>
                <Border
                    Grid.Column="1"
                    Classes="DefBorder">
                    <controls:GeneratedNPC
                        VerticalAlignment="Stretch"
                        HorizontalAlignment="Stretch"
                        Name="CurrentNpc">
                    </controls:GeneratedNPC>
                </Border>
                <Border
                    Classes="DefBorder"
                    Grid.Column="2">
                    <StackPanel>
                        <Button
                            Name="SaveNPC"
                            Click="SaveNPC_OnClick"
                            Margin="5"
                            VerticalAlignment="Center"
                            HorizontalAlignment="Stretch"
                            VerticalContentAlignment="Center"
                            HorizontalContentAlignment="Center"
                            Content="{DynamicResource Save}">
                        </Button>
                        <Button
                                Name="GenerateNPC"
                                Click="GenerateNPC_OnClick"
                                Margin="5"
                                VerticalAlignment="Center"
                                HorizontalAlignment="Stretch"
                                VerticalContentAlignment="Center"
                                HorizontalContentAlignment="Center"
                                Content="{DynamicResource Generate}">
                        </Button>
                        <ComboBox
                            Name="Age"
                            PlaceholderText="{DynamicResource Age}"
                            Margin="5"
                            VerticalAlignment="Center"
                            HorizontalAlignment="Stretch"
                            VerticalContentAlignment="Center"
                            HorizontalContentAlignment="Center">
                            <TextBlock
                                Name="a0a1001">
                                Random
                            </TextBlock>
                            <TextBlock
                                Name="a0a100"
                                Text="{DynamicResource Child}">
                                
                            </TextBlock>
                            <TextBlock
                                Name="a100a130"
                                Text="{DynamicResource Young}">
                            </TextBlock>
                            <TextBlock
                                Name="a130a250"
                                Text="{DynamicResource Youngadult}" >
                            </TextBlock>
                            <TextBlock
                                Name="a250a300"
                                Text="{DynamicResource Adult}" >
                            </TextBlock>
                            <TextBlock
                                Name="a300a600"
                                Text="{DynamicResource Middleaged}">
                            </TextBlock>
                            <TextBlock
                                Name="a600a1001"
                                Text="{DynamicResource Old}">
                            </TextBlock>
                        </ComboBox>
                        <ComboBox
                            Name="Racech"
                            PlaceholderText="Race"
                            Margin="5"
                            VerticalAlignment="Center"
                            HorizontalAlignment="Stretch"
                            VerticalContentAlignment="Center"
                            HorizontalContentAlignment="Center">
                            <TextBlock
                                Name="rnd">
                                Random
                            </TextBlock>
                            <TextBlock
                                Name="Gnome"
                                Text="Gnome">
                                
                            </TextBlock>
                            <TextBlock
                                Name="Dwarf"
                                Text="Dwarf">
                            </TextBlock>
                            <TextBlock
                                Name="Halforc"
                                Text="Half-orc" >
                            </TextBlock>
                            <TextBlock
                                Name="Halfling"
                                Text="Halfling" >
                            </TextBlock>
                            <TextBlock
                                Name="Halfelf"
                                Text="Half-elf">
                            </TextBlock>
                            <TextBlock
                                Name="Tiefling"
                                Text="Tiefling">
                            </TextBlock>
                            <TextBlock
                                Name="Human"
                                Text="Human">
                            </TextBlock>
                            <TextBlock
                                Name="Elf"
                                Text="Elf">
                            </TextBlock>
                        </ComboBox>

                    </StackPanel>
                </Border>
            </Grid>
        </TabItem>
        <TabItem
            IsEnabled="True">
            <TabItem.Header>
                <TextBlock
                    Text="{DynamicResource Loot}">
                    
                </TextBlock>
            </TabItem.Header>
             <Grid
                 ColumnDefinitions="*,*"
                 RowDefinitions="*,*">
                 <Border
                     Classes="DefBorder"
                     Grid.Row="0"
                     Grid.Column="0">
                     <DockPanel
                         LastChildFill="True">
                         <Grid
                             Name="FNPC"
                             ColumnDefinitions="*,*"
                             DockPanel.Dock="Top">
                             <TextBlock
                                 Margin="5"
                                 FontSize="20"
                                 VerticalAlignment="Center"
                                 HorizontalAlignment="Left">
                                 NPC
                             </TextBlock>
                             <StackPanel
                                 Height="{Binding #FNPC.Bounds.Height}"

                                 Grid.Column="1"
                                 VerticalAlignment="Center"
                                 HorizontalAlignment="Right"
                                 Orientation="Horizontal">
                                 <Button
                                     Click="Button_OnClick"
                                     VerticalAlignment="Stretch"
                                     Margin="0"
                                     CornerRadius="0,5,0,0">
                                     <PathIcon
                                         Data="{StaticResource settings_regular}">
                                     </PathIcon>
                                 </Button>
                             </StackPanel>
                         </Grid>
                         <Rectangle
                             Height="1"
                             Fill="DimGray"
                             DockPanel.Dock="Top">
                         </Rectangle>
