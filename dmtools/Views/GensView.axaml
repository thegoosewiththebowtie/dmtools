                                     VerticalAlignment="Center"
                                     HorizontalAlignment="Left"
                                     Grid.Row="0"
                                     Grid.Column="0">
                                     NPC's type
                                 </TextBlock>
                                 <ComboBox
                                     Name="NpcType"
                                     VerticalAlignment="Stretch"
                                     HorizontalAlignment="Stretch"
                                     Margin="5"
                                     Grid.Row="0"
                                     Grid.Column="1">
                                     <ComboBoxItem
                                         Name="bystander">
                                         Bystander
                                     </ComboBoxItem>
                                     <ComboBoxItem
                                         Name="bandit">
                                         Bandit
                                     </ComboBoxItem>
                                     <ComboBoxItem
                                         Name="madman">
                                         Local madman
                                     </ComboBoxItem>
                                     <ComboBoxItem
                                         Name="exad">
                                         Ex-adventurer
                                     </ComboBoxItem>
                                     <ComboBoxItem
                                         Name="magic">
                                         Local magician
                                     </ComboBoxItem>
                                     <ComboBoxItem
                                         Name="rich">
                                         Rich person
                                     </ComboBoxItem>
                                     <ComboBoxItem
                                         Name="poor">
                                         Poor citizen
                                     </ComboBoxItem>
                                     <ComboBoxItem
                                         Name="The">
                                         <TextBlock
                                             Width="{Binding #NpcType.Bounds.Width}"
                                             LineSpacing="-14"
