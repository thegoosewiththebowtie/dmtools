<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        CanResize="False"
        WindowStartupLocation="CenterOwner"
        Icon="/Assets/dmtools.ico"
        Height="255"
        Width="300"
        x:Class="dmtools.PopUps.AddCustNpc"
        Title="AddCustNpc">
        <Grid
                RowDefinitions="*,*,*,*,*,*">
                <TextBox
                        VerticalContentAlignment="Center"
                        HorizontalContentAlignment="Left"
                        VerticalAlignment="Stretch"
                        HorizontalAlignment="Stretch"
                        Grid.Row="0"
                        KeyDown="TestInt"
                        Name="Initiative"
                        Watermark="{DynamicResource lnggg}"
                        Margin="5"></TextBox>
                <TextBox
                        VerticalContentAlignment="Center"
                        HorizontalContentAlignment="Left"
                        VerticalAlignment="Stretch"
                        HorizontalAlignment="Stretch"
                        Grid.Row="1"
                        Name="Name"
                        Watermark="{DynamicResource NameS}"
                        Margin="5"></TextBox>
                <TextBox
                        VerticalContentAlignment="Center"
                        HorizontalContentAlignment="Left"
                        VerticalAlignment="Stretch"
                        HorizontalAlignment="Stretch"
                        Grid.Row="2"
                        Name="NPC"
                        IsEnabled="False"
                        Margin="5">
                        NPC
                </TextBox>
                <TextBox
                        VerticalContentAlignment="Center"
                        HorizontalContentAlignment="Left"
                        VerticalAlignment="Stretch"
                        HorizontalAlignment="Stretch"
                        Grid.Row="3"
                        KeyDown="TestInt"
                        Name="AC"
                        Watermark="{DynamicResource ArmorClass}"
                        Margin="5"></TextBox>
                <TextBox
                        VerticalContentAlignment="Center"
                        HorizontalContentAlignment="Left"
                        VerticalAlignment="Stretch"
                        HorizontalAlignment="Stretch"
                        Grid.Row="4"
                        KeyDown="TestInt"
                        Name="HP"
                        Watermark="{DynamicResource Health}"
                        Margin="5"></TextBox>
                <Grid
                        Grid.Row="6"
                        ColumnDefinitions="*, 2*, *, 2*, *">
                        <Button
                                Click="OK"
                                IsDefault="True"
                                Margin="5"
                                VerticalContentAlignment="Center"
                                HorizontalContentAlignment="Center"
                                VerticalAlignment="Stretch"
                                HorizontalAlignment="Stretch"
                                Grid.Column="1"
                                Content="{DynamicResource ADD}"></Button>
                        <Button
                                Click="Cancel"
                                Margin="5"
                                VerticalContentAlignment="Center"
                                HorizontalContentAlignment="Center"
                                VerticalAlignment="Stretch"
                                HorizontalAlignment="Stretch"
                                Grid.Column="3"
                                Content="{DynamicResource ToDoDelete}"></Button>
                </Grid>
        </Grid>
</Window>
